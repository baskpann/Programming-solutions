# Enter your code here. Read input from STDIN. Print output to STDOUT
n = int(input())
inp = []
even = 
odd = 
for i in range(n):
    inp.append(input())
for i in range(n):
    j = 0
    while j < len(inp[i]):
        if (j % 2) == 0:
            even += inp[i][j]
        else:
            odd += inp[i][j]
        j += 1
    print(even +   + odd)
